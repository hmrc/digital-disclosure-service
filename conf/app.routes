# microservice specific routes

->          /hmrc-frontend                               hmrcfrontend.Routes

GET         /assets/*file                                controllers.Assets.versioned(path="/public", file: Asset)

GET         /notification/store/user/:userId/id/:id      controllers.NotificationStoreController.get(userId, id)
GET         /notification/store/user/:userId             controllers.NotificationStoreController.getAll(userId)
PUT         /notification                                controllers.NotificationStoreController.set()
